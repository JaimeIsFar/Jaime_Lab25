% In this document you will write all the details about every piece of work you want to cite during your thesis project.
% There are several different formats which you can use to do so, for instance:

% If it is an article:

@article{Articulo,
	Abstract = {Resumen (Esta parte es opcional; sólo se los recomendaria porque sirve para tener un resumen de las cosas)},
	Author = {Autor},
	Journal = {Journal},
	Number = {3},
	Numpages = {9},
	Pages = {extension},
	Title = {Titulo del articuclo padriurix},
	Volume = {volumen},
	Year = {año}}

% If you're going to cite just a chapter:

@incollection{Capitulo,
  author      = {Autor de Libro},
  title       = {Titulo del Capitulo},
  booktitle   = {Titulo del Libro},
  publisher   = {Editorial},
  address     = {Direccion},
  year        = {1993},
  pages       = {Paginas},
  chapter     = {Cap}}

 %P If you're going to cite an entire book

  @book{Fechner,
  author      = {G. T. Fechner},
  editor      = {"E. G. Boring and D. H. Howes"},
  title       = {"Elements of psychophysics"},
  volume      = {1},
  translator  = {H. E. Adler},
  publisher   = {"New York : Holt, Rinehart \& Winston"},
  address     = {"New York"},
  origdate    = {1860},
  year        = {1966}}

%Or if you want to cite a website
@ONLINE{Pagina del Lab,
author = {Laboratorio 25},
title = {Sitio oficial del lab},
year = {2017},
url = {https://bouzaslab25.github.io}}

%Several other types of formats for citing can be found on the internet if you just google "Latex bib... and the format you're interested in"